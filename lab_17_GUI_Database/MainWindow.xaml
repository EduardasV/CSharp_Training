<Window x:Class="lab_17_GUI_Database.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab_17_GUI_Database"
        mc:Ignorable="d"
        Title="MainWindow" Height="auto" Width="auto">
    <Grid Margin="0,0,-0.4,-0.4">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="10*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Button x:Name="Button01" Background="Aqua" Grid.Row="0" Grid.Column="0" Click="Button01_Click" />
        <ListBox x:Name="ListBox01" Background="Beige" Grid.Row="2" Grid.Column="1" />
    </Grid>
</Window>
